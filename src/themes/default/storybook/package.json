    "@storybook/addon-docs": "7.0.27",
    "@storybook/addon-interactions": "7.0.27",
    "@storybook/addon-viewport": "7.0.27",
    "@storybook/builder-webpack5": "7.0.27",
    "@storybook/html": "7.0.27",
