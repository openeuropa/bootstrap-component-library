  "version": "0.23.1",
  "description": "OE storybook",
  "scripts": {
    "build": "npm run clean && npm run copy",
    "clean": "rimraf bcl-stories",
    "copy": "cross-env bcl-builder copy",
    "postinstall": "npm run copy",
    "start": "start-storybook -p 9001 -c .storybook",
    "dist": "build-storybook -c .storybook -o ../../../../build/default",
    "dist-chromatic": "STORYBOOK_ENV=chromatic build-storybook -c .storybook -o ../../../../build/default"
  },
  "devDependencies": {
    "@babel/core": "7.17.12",
    "@babel/parser": "7.17.12",
    "@babel/preset-env": "7.17.12",
    "@babel/preset-react": "7.17.12",
    "@babel/runtime": "7.17.9",
    "@geometricpanda/storybook-addon-badges": "0.2.1",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-controls": "6.4.22",
    "@storybook/addon-docs": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-viewport": "6.4.22",
    "@storybook/html": "6.4.22",
    "@storybook/jest": "0.0.10",
    "@storybook/testing-library": "0.0.11",
    "@whitespace/storybook-addon-code": "1.0.2",
    "babel-loader": "8.2.5",
    "core-js": "3.22.5",
    "drupal-attribute": "1.0.2",
    "null-loader": "4.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "rimraf": "3.0.2",
    "storybook-addon-designs": "6.2.1",
    "twing": "2.3.7",
