    "@storybook/builder-webpack5": "9.1.4",
    "@storybook/html": "9.1.4",
    "@storybook/jest": "0.2.3",
    "babel-loader": "10.0.0",
    "buffer": "6.0.3",
    "core-js": "3.45.1",
    "cross-env": "10.0.0",
    "drupal-attribute": "1.1.0",
    "null-loader": "4.0.1",
    "raw-loader": "4.0.2",
    "react": "19.1.1",
